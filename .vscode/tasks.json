  "version": "2.0.0",
  "tasks": [
    {
      "label": "Complete build",
      "type": "deno",
      "command": "task",
      "args": ["build"],
      "group": "build",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      }
    },
    {
      "label": "Cargo build",
      "type": "deno",
      "command": "task",
      "args": ["cargo-build"],
      "group": "build",
      "presentation": {
        "reveal": "silent",
        "panel": "shared"
      }
    }
  ]
}
